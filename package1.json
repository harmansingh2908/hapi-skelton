  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgnivaIT/NextFoodie_Web.git"
  },
  "keywords": ["node", "hapi", "js", "graphql", "mongo"],
  "bugs": {
    "url": "https://github.com/IgnivaIT/NextFoodie_Web/issues"
  },
  "homepage": "https://github.com/IgnivaIT/NextFoodie_Web#readme",
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "babel-html": "^0.5.1",
    "bluebird": "^3.5.1",
    "boom": "^6.0.0",
    "config": "1.26.2",
    "good": "^8.0.0-rc1",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "handlebars": "^4.0.11",
    "hapi": "^17.1.1",
    "http-status": "1.0.1",
    "inert": "^5.0.1",
    "jsonwebtoken": "8.1.0",
    "lodash": "^4.17.4",
    "md5": "2.2.1",
    "moment": "^2.19.1",
    "mongoose": "^4.12.4",
    "nodemailer": "4.2.0",
    "nodemailer-smtp-transport": "2.7.4",
    "npm": "^5.7.1",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.16",
    "querystring": "^0.2.0",
    "slugify": "^1.2.9",
    "socket.io": "2.0.4",
    "stripe": "5.1.1",
    "subscriptions-transport-ws": "^0.9.4",
    "underscore": "1.8.3",
    "uuid": "^3.1.0",
    "vision": "^5.2.0",
    "webshot": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bunyan": "^2.0.2",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "joi": "^13.0.1",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.2"
  },
  "main": "main.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- main.js",
    "dev": "export NODE_ENV=dev && nodemon --exec babel-node -- main.js",
    "staging": "export NODE_ENV=staging && node dist/main.js",
    "uk_local": "export NODE_ENV=uk_local && nodemon --exec babel-node -- main.js",
    "serve-fs": "npm run ri && export NODE_ENV=staging && pm2 start dist/main.js --name=nf",
    "serve-rs": "pm2 restart nf",
    "serve-ns": "pm2 delete nf && npm run serve-fs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && babel . --ignore node_modules,web_admin,updated_modules --out-dir dist",
    "ri": "rm -rf node_modules && npm install",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "rp": "prettier --write \"**/*.json\" \"**/*.js\" \"!updated_modules/**/*.js\" \"!web_admin/**/*.js\" \"!web_admin/**/*.json\"  \"!dist/**/*.js\"",
    "precommit": "npm run rp && lint-staged"
  },
  "author": "Hermenpreet"
}
